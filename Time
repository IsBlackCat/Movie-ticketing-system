package traning2;

import java.util.Scanner;

public class Time {
	String time;
	Pos pos = new Pos();
	Scanner sc = new Scanner(System.in);
	public void initial() {
		System.out.println("输入片场时间：");
		time = sc.next();
		pos.initial();
	}
	
	public String getTime() {
		return this.time;
	}
	public void setTime(String time) {
		this.time = time;
	}
}
